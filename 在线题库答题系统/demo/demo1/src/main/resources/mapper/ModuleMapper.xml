<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo2.mapper.ModuleMapper">

    <resultMap id="BaseResultMap" type="com.example.demo2.entity.po.Module">
            <id property="moduleId" column="module_id" jdbcType="VARCHAR"/>
            <result property="lessonId" column="lesson_id" jdbcType="VARCHAR"/>
            <result property="moduleName" column="module_name" jdbcType="VARCHAR"/>
            <result property="lessonVideoUrl" column="lesson_video_url" jdbcType="VARCHAR"/>
            <result property="lessonDetail" column="lesson_detail" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        module_id,lesson_id,module_name,
        lesson_video_url,lesson_detail
    </sql>
</mapper>
